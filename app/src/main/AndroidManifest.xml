            android:theme="@style/Theme.LetsCompose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_true_main2"
            android:theme="@style/Theme.LetsCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".places.codelabbasics.CodelabBasicsActivity"
            android:exported="false"
            android:label="@string/title_activity_codelab_basics"
            android:theme="@style/Theme.LetsCompose" />
        <activity
            android:name=".places.survey.SurveyActivity"
            android:exported="false"
            android:label="@string/title_activity_survey"
            android:theme="@style/Theme.LetsCompose" />
        <activity
            android:name=".places.Conversation.ConversationActivity"
            android:exported="false"
            android:theme="@style/Theme.LetsCompose" />
    </application>

</manifest>